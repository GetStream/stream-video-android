<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CallAppBar">
        <attr name="streamCallAppBarBackgroundColour" format="color|reference" />
        <attr name="streamCallAppBarBackgroundColourLandscape" format="color|reference" />
        <attr name="streamCallAppBarPadding" format="dimension|reference" />

        <attr name="streamCallAppBarLeadingContentIcon" format="reference" />
        <attr name="streamCallAppBarLeadingContentIconTint" format="color|reference" />
        <attr name="streamCallAppBarLeadingContentIconTintLandscape" format="color|reference" />
        <attr name="streamCallAppBarLeadingContentMarginStart" format="dimension|reference" />
        <attr name="streamCallAppBarLeadingContentMarginEnd" format="dimension|reference" />

        <attr name="streamCallAppBarCenterContentTextSize" format="dimension|reference" />
        <attr name="streamCallAppBarCenterContentTextColor" format="color|reference" />
        <attr name="streamCallAppBarCenterContentFontAsset" format="reference" />
        <attr name="streamCallAppBarCenterContentFont" format="reference" />
        <attr name="streamCallAppBarCenterContentTextStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="streamCallAppBarCenterContentTextSizeLandscape" format="dimension|reference" />
        <attr name="streamCallAppBarCenterContentTextColorLandscape" format="color|reference" />
        <attr name="streamCallAppBarCenterContentFontAssetLandscape" format="reference" />
        <attr name="streamCallAppBarCenterContentFontLandscape" format="reference" />
        <attr name="streamCallAppBarCenterContentTextStyleLandscape">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="streamCallAppBarCenterContentMarginStart" format="dimension|reference" />
        <attr name="streamCallAppBarCenterContentMarginEnd" format="dimension|reference" />

        <attr name="streamCallAppBarTrailingContentIcon" format="reference" />
        <attr name="streamCallAppBarTrailingContentIconTint" format="color|reference" />
        <attr name="streamCallAppBarTrailingContentIconTintLandscape" format="color|reference" />
        <attr name="streamCallAppBarTrailingContentMarginStart" format="dimension|reference" />
        <attr name="streamCallAppBarTrailingContentMarginEnd" format="dimension|reference" />
    </declare-styleable>
</resources>